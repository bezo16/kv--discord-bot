{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "semi": 0,
        "comma-dangle": "off",
        "no-console": ["error", { "allow": ["warn", "error"] }]
    }
}
